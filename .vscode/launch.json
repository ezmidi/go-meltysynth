{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "No Arguments",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/_example/main.go"
        },
        {
            "name": "Chord Test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/_example/main.go",
            "args": ["-midi", "chord", "-o", "${workspaceFolder}/chord.pcm", "-sf2", "${workspaceFolder}/TimGM6mb.sf2"]
        },
        {
            "name": "MIDI Synth Test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/_example/main.go",
            "args": ["-midi", "${workspaceFolder}/flourish.mid", "-o", "${workspaceFolder}/flourish.pcm", "-sf2", "${workspaceFolder}/TimGM6mb.sf2"]
        }
    ]
}
